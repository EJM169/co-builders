<% include ./partials/contractorHeader %>
<!--================Header Menu Area =================-->
  <section id="Dashboard" class="dash_banner_area">
        <div class="container">
            <h1>List of Customers that contractor</h1>
         

            <% if(currentUser.project.length==1) { %>
                <div class=" card dash_content">
                    <div class="card-body align-items-center">
                        <h3>Name: <%=customerDetail.username%></h3>
                        <p>Email: <%=customerDetail.email%></p>
                        <p>Email: <%=customerDetail.mobile%></p>
                        <p>Experience: <%=customerDetail.requirements%></p>
                        <!-- <a href="/customer/contractor//"><input type="submit" class="btn btn-primary btn-lg" value="See More"> -->
                        <!-- </a> -->
                        <form action="#" method="post">
                            <button type="button" class="btn btn-outline-success"><i class="far fa-check-circle"></i></button>
                        </form>
                        <form action="#" method="post">
                            <button type="button" class="btn btn-outline-danger"><i class="fas fa-times"></i></button>
                        </form>
                    </div>
                </div> 
            <%} else {%>
            <%customerDetail.forEach(function(customerDetail){ %>

         
            <div class=" card dash_content">
                <div class="card-body align-items-center">
                    <h3>Name: <%=customerDetail.username%></h3>
                    <p>Email: <%=customerDetail.email%></p>
                    <p>Email: <%=customerDetail.mobile%></p>
                    <p>Experience: <%=customerDetail.requirements%></p>
                    <!-- <a href="/customer/contractor//"><input type="submit" class="btn btn-primary btn-lg" value="See More"> -->
                    <!-- </a> -->
                    <form action="#" method="post">
                        <button type="button" class="btn btn-outline-success"><i class="far fa-check-circle"></i></button>
                    </form>
                    <form action="#" method="post">
                        <button type="button" class="btn btn-outline-danger"><i class="fas fa-times"></i></button>
                    </form>
                </div>
            </div> 
            <%}); } %>  
        </div>
    </section>
    


